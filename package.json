{
  "name": "FbisMobile",
  "version": "0.7.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 14 Pro'",
    "start": "react-native start",
    "upgrade": "react-native upgrade",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "react-native setup-ios-permissions && pod-install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.2",
    "@react-native-community/datetimepicker": "^6.7.5",
    "@react-native-community/netinfo": "^9.4.1",
    "@react-native-picker/picker": "github:FonDorn/picker#chore/react18",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@react-navigation/stack": "^6.3.17",
    "@rneui/base": "^0.0.0-edge.2",
    "@rneui/themed": "^0.0.0-edge.2",
    "apisauce": "^2.1.6",
    "axios": "^0.27.2",
    "deprecated-react-native-prop-types": "^2.3.0",
    "formik": "^2.4.3",
    "mobx": "^6.10.0",
    "mobx-state-tree": "^5.1.8",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-native": "^0.71.12",
    "react-native-autocomplete-input": "^5.3.2",
    "react-native-camera": "github:dimasciput/react-native-camera#18",
    "react-native-config": "^1.5.1",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-fs": "^2.20.0",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-maps": "^1.7.1",
    "react-native-paper": "^4.12.5",
    "react-native-permissions": "^3.8.4",
    "react-native-progress": "^5.0.0",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "^3.15.0",
    "react-native-svg": "^13.11.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-vector-image": "^0.3.3",
    "rn-fetch-blob": "^0.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/runtime": "^7.22.10",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.3",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.69.1",
    "@types/react-native-autocomplete-input": "^5.1.1",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "axios-mock-adapter": "^1.21.5",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.4",
    "pod-install": "^0.1.38",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.9.5"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js",
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "testPathIgnorePatterns": [
      "__tests__"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@?react-native|@react-native-community|@react-navigation|@rneui)"
    ],
    "transform": {
      "\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "reactNativePermissionsIOS": [
    "Camera",
    "LocationAccuracy",
    "LocationAlways",
    "LocationWhenInUse"
  ],
  "reactNativePermissions": {
    "ios": [
      "Camera",
      "LocationAccuracy",
      "LocationAlways",
      "LocationWhenInUse"
    ]
  }
}
